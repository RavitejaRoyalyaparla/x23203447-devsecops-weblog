files:
  "/etc/nginx/conf.d/static.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      location /static/ {
          alias /var/app/current/static/;
          expires 1y;
      }

      location /media/ {
          alias /var/app/current/media/;
          expires 1y;
      }
